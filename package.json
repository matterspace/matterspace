{
  "name": "indiejobs",
  "version": "0.0.1",
  "description": "Freelancer job search made ridiculously easy",
  "main": "index.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "cross-env NODE_ENV=development ts-node ./src/server/index.ts",
    "start:watch": "cross-env NODE_ENV=development nodemon --watch src --exec \"ts-node ./src/server/index.ts\"",
    "hot-server": "webpack-dev-server --hot --config ./webpack.config.dev.js",
    "test": "cross-env NODE_ENV=test mocha ./test/unit/**/*.ts ",
    "test:integration": "cross-env NODE_ENV=test mocha ./test/integration/*Spec.ts",
    "test:all": "cross-env NODE_ENV=test mocha ./test/unit/*Spec.ts ./test/integration/*Spec.ts",
    "test:p": "cross-env NODE_ENV=test mocha",
    "tslint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "tslint:fix": "tslint ./src/**/*.ts ./test/**/*.ts --fix",
    "db:install": "cross-env COMMAND=create_dev_dbs PGPASSWORD=indiejobs ts-node ./tools/db-manager.ts",
    "db:drop": "cross-env COMMAND=drop_dev_dbs PGPASSWORD=indiejobs ts-node ./tools/db-manager.ts",
    "db:clean": "cross-env COMMAND=clean_up_dev_dbs PGPASSWORD=indiejobs ts-node ./tools/db-manager.ts",
    "db:generate": "cross-env COMMAND=generate_scripts PGPASSWORD=indiejobs ts-node ./tools/db-manager.ts",
    "db:reinstall": "npm run db:drop && npm run db:install",
    "tools:save-professions": "cross-env NODE_ENV=development ts-node ./tools/save-professions.ts",
    "tools:validate-professions": "cross-env NODE_ENV=development ts-node ./tools/validate-professions.ts",
    "tools:print-config": "ts-node ./tools/print-config.ts",
    "tools:massive-repl": "massive -c postgres://indiejobs:indiejobs@localhost:5432/indiejobs",
    "tools:apply-feminine-to-professions": "ts-node ./tools/apply-feminine-to-professionals.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrerpena/radiscope.git"
  },
  "author": "Andre Pena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrerpena/radiscope/issues"
  },
  "homepage": "https://github.com/andrerpena/radiscope#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-preset-es2016": "^6.24.1",
    "body-parser": "^1.17.2",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.2",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "massive": "^3.0.0",
    "normalize.css": "^7.0.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "react": "^15.6.1",
    "react-activity": "^1.1.2",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.5",
    "react-toggle-button": "^2.1.0",
    "redux": "^3.7.1",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.1",
    "@types/colors": "^1.1.3",
    "@types/cookie-parser": "^1.3.30",
    "@types/cookie-session": "^2.0.32",
    "@types/express": "^4.0.36",
    "@types/massive": "^3.0.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.9",
    "@types/passport": "^0.3.3",
    "@types/passport-google-oauth": "^1.0.35",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.35",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.45",
    "@types/react-router": "^4.0.13",
    "@types/react-router-dom": "^4.0.6",
    "@types/react-select": "^1.0.48",
    "@types/redux-form": "^6.6.6",
    "@types/webpack": "^3.0.2",
    "awesome-typescript-loader": "^3.2.1",
    "chai": "^4.0.2",
    "child-process-promise": "^2.2.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fs-extra-promise": "^1.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm": "^5.1.0",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "underscore": "^1.8.3",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  }
}
